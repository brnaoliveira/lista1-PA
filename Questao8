#include <stdio.h>
int main(){
  int vet[] = {4,9,13};
  int i;
  for(i=0;i<3;i++){
    printf("%d ",*(vet+i)); // imprime 4,9,13
  }
}

int main(){
  int vet[] = {4,9,13};
  int i;
  for(i=0;i<3;i++){
    printf("%X ",vet+i); // imprime os endereços de onde estão armazenado cada elemento do vetor
  }
}
